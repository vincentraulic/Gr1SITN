<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/login.jsf" />
	</url-mapping>
	
	<url-mapping id="home">
		<pattern value="/user/home" />
		<view-id value="/view/home/homeContent.jsf" />
	</url-mapping>
	
	<url-mapping id="schedule">
		<pattern value="/user/schedule" />
		<view-id value="/view/schedule.jsf" />
	</url-mapping>
	
	<url-mapping id="newuser">
		<pattern value="/user/newuser" />
		<view-id value="/view/newUserContent.jsf" />
	</url-mapping>	
	
	<url-mapping id="newproject">
		<pattern value="/user/newproject" />
		<view-id value="/view/newProject.jsf" />
	</url-mapping>	
	
	<url-mapping id="projectbudget">
		<pattern value="/user/projectbudget" />
		<view-id value="/view/projectbudget/projectbudget.jsf" />
	</url-mapping>		
	
	<url-mapping id="taskaffectation">
		<pattern value="/user/taskaffectation" />
		<view-id value="/view/projectmanagement/task_employee.jsf" />
	</url-mapping>		
	
	<url-mapping id="taskcreation">
		<pattern value="/user/taskcreation" />
		<view-id value="/view/projectmanagement/taskcreation.jsf" />
	</url-mapping>	

</pretty-config>